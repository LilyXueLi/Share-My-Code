{
    "name": "Share My Leetcode",
    "description": "Share your awesome leetcode with friends within one click!",
    "version": "0.1.1",
    "manifest_version": 2,
    "icons": {
        "16": "./Logos/logo_large.png"
    },
    "background": {
        "scripts": [
            "./background.js"
        ]
    },
    "options_page": "./options.html",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "https://leetcode.com/problems/",
        "storage"
    ]
}